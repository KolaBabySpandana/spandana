 //Program to print given string is palindrome or not without using string2 & without using string functions
#include <stdio.h>
#include <string.h>
int main() {
    char str[20];
    int len,i=0,j;
    gets(str);
    len=strlen(str);
    j=len-1;
    while(i<len/2){
        char temp;
        temp=str[i];
        str[i]=str[j];
        str[j]=temp;
        i++;
        j--;
    }
    if(str[i]==str[j])
    printf("Palindrome");
    else
    printf("Not Palindrome");
    return 0;
}
