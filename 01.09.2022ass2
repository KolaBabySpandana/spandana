//Write a C Program to find FACTORIAL of a number using functions with return value
#include <stdio.h>
int main() {
    int x,ans;
    printf("enter a number");
    scanf("%d",&x);
    ans=cal(x);
    printf("factorial=%d",ans);
}
int cal(int x){
	int i,fact=1;
	for(i=1;i<x;i++){
		fact=fact*i;
	}
}
