#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char *s;
    s = malloc(1024 * sizeof(char));
    int i,len;
    scanf("%[^\n]",s);
    len=strlen(s);
    for(i=0;i<len;i++){
        if(s[i]==' ')
        printf("\n");
        else 
        printf("%c",s[i]);
    }
    return 0;
}
